{
    "manifest_version": 2,
    "name": "Window Image Downloader",
    "version": "1.0",
    "description": "Download pictures from multiple pages",
    "permissions": ["activeTab", "tabs",  "*://*/*", "downloads"],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon-16.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
      }
    }
  }
  